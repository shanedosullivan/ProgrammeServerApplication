<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.programme.server.mapper.RegistrationMapper">
	
	<insert id="sendTokenToServer" parameterType="String">
	    INSERT INTO PROGRAMME.REGISTRATION (TOKEN)
	    SELECT #{token}
	    WHERE
	    	NOT EXISTS(
	    		SELECT #{token} FROM PROGRAMME.REGISTRATION WHERE (TOKEN) = #{token}
	    	);
	</insert>
	
	<select id="retrieveAllTokens" resultType="string">
	    SELECT TOKEN FROM PROGRAMME.REGISTRATION;
	</select>
</mapper>